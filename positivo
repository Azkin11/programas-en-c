#include <stdio.h>

int main(void) {
    int numero;

    printf("Introduzca un nÃºmero: ");
    scanf("%d", &numero);

    if (numero > 0) {
        printf("Es mayor que 0\n");
    } else if (numero < 0) {
        printf("Es menor que 0\n");
    } else {
        printf("Es 0\n");
    }

    return 0;
}

